<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity File.
    -->
    <changeSet id="20200707070901-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="motel_id"
                                 baseTableName="file"
                                 constraintName="fk_file_motel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="motel"/>

        <addForeignKeyConstraint baseColumnNames="room_id"
                                 baseTableName="file"
                                 constraintName="fk_file_room_id"
                                 referencedColumnNames="id"
                                 referencedTableName="room"/>

    </changeSet>
</databaseChangeLog>
